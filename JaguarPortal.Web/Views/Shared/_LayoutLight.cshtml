<!DOCTYPE html>
<html lang="en">
<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Jaguar Portal">
    <meta name="author" content="Erickson Lima">

    <title>JAGUAR PORTAL - @Html.Raw(ViewData["Title"])</title>

    <!-- Custom fonts for this template-->
    <link href="~/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css" asp-append-version="true">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet" asp-append-version="true">

    <!-- Custom styles for this template-->
    <link href="~/css/sb-admin-2.min.css" rel="stylesheet" asp-append-version="true">
    <link href="~/css/site.css" rel="stylesheet" asp-append-version="true">
    <link href="~/css/stackoverflow-light.css" rel="stylesheet" asp-append-version="true">
    <link href="~/vendor/datatables/dataTables.bootstrap4.min.css" rel="stylesheet" asp-append-version="true">

    <link rel="icon" href="~/img/jaguar-icon.png" asp-append-version="true" />

    <link rel="stylesheet" href="~/css/test-requirement-code-template.css" asp-append-version="true">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@1,400;1,500&display=swap">
    @await RenderSectionAsync("Styles", required: false)
</head>

<body id="page-top">

    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- End of Sidebar -->
        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">
            <!-- Main Content -->
            <div id="content">

                <main>

                        @RenderBody()
                </main>
            </div>

            <!-- End of Main Content -->
            <!-- Footer -->
            <footer class="sticky-footer bg-white">
                <div class="container my-auto">
                    <div class="copyright text-center my-auto">
                        <span>Copyright &copy; Jaguar Portal @Html.Raw(DateTime.Now.Year)</span>
                    </div>
                </div>
            </footer>
            <!-- End of Footer -->
            <!-- End of Content Wrapper -->
        </div>
    </div>

    <!-- End of Page Wrapper -->
    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>






    <!-- Bootstrap core JavaScript-->
    <script src="~/vendor/jquery/jquery.min.js" asp-append-version="true"></script>
    <script src="~/vendor/bootstrap/js/bootstrap.bundle.min.js" asp-append-version="true"></script>

    <!-- Core plugin JavaScript-->
    <script src="~/vendor/jquery-easing/jquery.easing.min.js" asp-append-version="true"></script>

    <!-- Custom scripts for all pages-->
    <script src="~/js/sb-admin-2.min.js" asp-append-version="true"></script>

    <!-- Page level plugins -->
    <script src="~/vendor/chart.js/Chart.min.js" asp-append-version="true"></script>
    <script src="~/vendor/datatables/jquery.dataTables.min.js" asp-append-version="true"></script>
    <script src="~/vendor/datatables/dataTables.bootstrap4.min.js" asp-append-version="true"></script>


    <!-- Page level custom scripts -->
    @*<script src="js/demo/chart-area-demo.js"></script>
    <script src="js/demo/chart-pie-demo.js"></script> *@

    @*<script src="~/lib/jquery/dist/jquery.min.js" asp-append-version="true"></script>*@
    @*<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js" asp-append-version="true"></script>*@
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>

