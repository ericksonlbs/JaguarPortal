@model List<JaguarWeb.Clients.AnalysisControlFlowModel>
@{
    ViewData["Title"] = "Projects";
}


        
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">List Analysed Projects</h6>
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <div id="dataTable_wrapper" class="dataTables_wrapper dt-bootstrap4">
                    <div class="col-sm-12">
                    <table class="table table-bordered table-striped dataTable" id="dataTable" width="100%" cellspacing="0" role="grid" aria-describedby="dataTable_info" style="width: 100%;">
                            <thead>
                                <tr role="row">
                                    <th>Project</th>
                                    <th>Last Analyzed</th>
                                </tr>

                            </thead>
                            <tbody>
                                @foreach (var item in Model.GroupBy(x => x.ProjectID))
                                {                                    
                                    <tr>
                                        <td><a href="~/Project/@item.Key">@item.Key </a></td>
                                        <td><a href="~/Project/@item.Key">@Model.Where(x=>x.ProjectID == item.Key).Max(x=>x.CreatedDate).ToString("MM/dd/yyyy HH:mm")</a></td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>

            </div>
        </div>
    </div>


@section scripts {
    <script src="~/js/demo/datatables-demo.js" asp-append-version="true"></script>
                    }